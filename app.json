{
  "expo": {
    "name": "Missfits Scouting App",
    "slug": "scouting-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconAndroid.png",
    "primaryColor": "#03b0ff",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.missfitsrobotics.missfitsscouting",
      "icon": "./assets/images/iconAppStore.png"
    },
    "android": {
      "package": "com.missfitsrobotics.missfitsscouting",
      "versionCode": 2,
      "icon": "./assets/images/iconAndroid.png",
      "permissions": []
    }
  }
}